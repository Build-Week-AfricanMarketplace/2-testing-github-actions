{
  "description": "Aileron Components Web",
  "private": true,
  "engines": {
    "node": ">=14.17.0"
  },
  "repository": {
    "url": "https://github.com/AAInternal/aileron",
    "type": "git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap -- --legacy-peer-deps --registry https://packages.aa.com/artifactory/api/npm/npm-public/",
    "link": "lerna link",
    "build": "lerna run build --concurrency 3",
    "build:since": "nx affected --target=build",
    "build:prod": "lerna run build:prod --concurrency 3 --skip-nx-cache",
    "build:prod:since": "nx affected --target=build:prod",
    "build:tokens": "lerna run build --stream --scope @aileron/styles --skip-nx-cache",
    "dev": "npx concurrently -n build,storybook \"npm:start\" \"npm:storybook\"",
    "lint:ts": "lerna run lint --concurrency 3",
    "lint:ts:since": "nx affected --target=lint:ts",
    "lint:css": "lerna run lint:css --concurrency 3",
    "lint:css:since": "nx affected --target=lint:css",
    "postinstall": "npm run bootstrap",
    "prepack": "npm run build:prod",
    "prestorybook": "npm run build && npm run wca && npx postcss .storybook/globals.css -o .storybook/globals.out.css",
    "storybook": "start-storybook -p 8180 --no-manager-cache",
    "prebuild-storybook": "npm run wca && npm run build && npx postcss .storybook/globals.css -o .storybook/globals.out.css",
    "build-storybook": "build-storybook -o docs",
    "predeploy-storybook": "npm run build-storybook",
    "deploy-storybook": "storybook-to-ghpages --existing-output-dir ./docs",
    "start": "npm run watch",
    "watch": "lerna run watch --parallel",
    "generate": "plop --plopfile scripts/plopfile.js",
    "postgenerate": "npm run bootstrap",
    "wca": "del-cli ./custom-elements.json && npx web-component-analyzer analyze \"src/components/**/src/*.ts\" --outFile custom-elements.json",
    "prepare": "husky install",
    "test": "lerna run test --concurrency 5 --stream",
    "test:since": "nx affected --target=test",
    "coverage": "node ./scripts/aggregate-coverage.js && nyc report --reporter lcov",
    "postcoverage": "npx --yes http-server ./coverage/lcov-report -o",
    "test-storybook": "test-storybook --url http://localhost:8180",
    "test:generate-output": "jest --config jest.config.storybook.js --json --outputFile=.jest-test-results.json"
  },
  "resolutions": {
    "lit-html": "^2.2.6"
  },
  "dependencies": {
    "@floating-ui/dom": "^0.5.4",
    "flatpickr": "^4.6.9",
    "focus-visible": "^5.2.0",
    "innersvg-polyfill": "^0.0.5",
    "lit": "^2.1.1",
    "lit-html": "^2.2.6",
    "style-dictionary": "^3.7.0",
    "ts-dedent": "^2.2.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@custom-elements-manifest/analyzer": "^0.5.2",
    "@lerna/conventional-commits": "^4.0.0",
    "@mdx-js/react": "~1.6.22",
    "@octokit/rest": "^19.0.3",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-a11y": "^6.5.8",
    "@storybook/addon-docs": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-jest": "^6.5.12",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/test-runner": "^0.1.1",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.8",
    "@storybook/web-components": "^6.5.8",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/node": "^16.7.8",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^29.0.3",
    "conventional-changelog-cli": "^2.1.1",
    "cp-file": "^9.1.0",
    "cspell": "^5.19.1",
    "custom-elements-manifest": "^1.0.0",
    "del-cli": "^3.0.1",
    "esbuild": "^0.15.10",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^2.2.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-wc": "^1.3.2",
    "globby": "^13.1.2",
    "husky": "^7.0.4",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-runner-stylelint": "^2.3.7",
    "lerna": "^5.1.1",
    "lint-staged": "^12.3.3",
    "lit-analyzer": "^1.2.1",
    "lodash.kebabcase": "^4.1.1",
    "nx": "^14.1.9",
    "nyc": "^15.1.0",
    "plop": "^3.1.0",
    "postcss": "^8.4.1",
    "postcss-cli": "^10.0.0",
    "postcss-lit": "^0.4.3",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-syntax-highlighter": "^15.5.0",
    "rollup": "^2.76.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-lit-css": "^4.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "semver": "^7.3.5",
    "serialize-javascript": "^6.0.0",
    "sinon": "^13.0.1",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "tinycolor2": "^1.4.2",
    "ts-jest": "^29.0.1",
    "ts-lit-plugin": "^1.2.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "4.5.5",
    "webpack": "^5.73.0"
  },
  "lint-staged": {
    "src/components/src/*.{ts,js}": [
      "eslint --max-warnings 0 --cache --fix",
      "prettier --write"
    ]
  },
  "customElements": "custom-elements.json",
  "workspaces": [
    "src/components/*"
  ]
}
